<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.FixTeamScheduleController">
   <children>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backOnClick" prefHeight="25.0" prefWidth="41.0" text="back" />
      <Label layoutX="77.0" layoutY="73.0" text="List of Activity">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="465.0" layoutY="133.0" text="Role :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="465.0" layoutY="276.0" text="Activity :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="466.0" layoutY="308.0" text="Time Start :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="466.0" layoutY="342.0" text="Time Stop :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="467.0" layoutY="238.0" text="Operator :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseOperator" layoutX="536.0" layoutY="236.0" onAction="#chooseWhichOperator" prefHeight="20.0" prefWidth="150.0" promptText="Choose Operator" />
      <Label fx:id="timeStartLabel" layoutX="545.0" layoutY="308.0" text="Time Start">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="timeStopLabel" layoutX="545.0" layoutY="342.0" text="Time Stop">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="499.0" layoutY="381.0" mnemonicParsing="false" onAction="#updateDataToTarget" prefHeight="25.0" prefWidth="93.0" text="Finish" />
      <Label fx:id="constantTeamLabel" layoutX="467.0" layoutY="200.0" text="Team :">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseTeam" layoutX="517.0" layoutY="197.0" onAction="#chooseWhichTeam" prefWidth="150.0" promptText="Choose Team" />
      <Label fx:id="nameLabel" layoutX="524.0" layoutY="276.0" text="Activity name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TableView fx:id="activityTableView" layoutX="55.0" layoutY="120.0" prefHeight="444.0" prefWidth="366.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <RadioButton fx:id="chooseRoleSingleParticipant" layoutX="510.0" layoutY="135.0" mnemonicParsing="false" onAction="#selectedParticipant" text="singlely participants" />
      <RadioButton fx:id="chooseRoleTeam" layoutX="510.0" layoutY="164.0" mnemonicParsing="false" onAction="#selectedTeam" text="Event organizer team" />
   </children>
</AnchorPane>
