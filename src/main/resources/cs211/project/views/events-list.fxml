            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="517.0" layoutY="370.0" mnemonicParsing="false" onAction="#onApplyToStaffClick" prefWidth="119.0" stylesheets="@othersButton.css" text="apply for staff" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
