            <TextField fx:id="searchTextField" alignment="TOP_LEFT" prefWidth="300.0" promptText="Type here to search." styleClass="search-box" />
            <Button mnemonicParsing="false" onAction="#onSearchEventButton" prefHeight="15.0" prefWidth="90.0" styleClass="search-button" text="Search" textFill="WHITE">
