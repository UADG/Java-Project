         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="387.0" layoutY="290.0" mnemonicParsing="false" prefWidth="119.0" text="apply for staff">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="387.0" layoutY="325.0" mnemonicParsing="false" prefWidth="119.0" text="apply for participant">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackClick" text="Back" />
   </children>
</AnchorPane>
