      <Button layoutX="411.0" layoutY="420.0" mnemonicParsing="false" onAction="#onBookTicketsClick" prefHeight="24.0" prefWidth="96.0" stylesheets="@othersButton.css" text="Book tickets" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
